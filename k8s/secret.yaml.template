# DO NOT COMMIT THIS FILE WITH REAL VALUES
# Copy to secret.yaml and fill in values, or use kubectl create secret

apiVersion: v1
kind: Secret
metadata:
  name: voice-call-mcp-secrets
type: Opaque
stringData:
  DATABASE_URL: "postgresql://user:password@host:5432/voicecall"
  TWILIO_ACCOUNT_SID: "your-twilio-account-sid"
  TWILIO_AUTH_TOKEN: "your-twilio-auth-token"
  TWILIO_NUMBER: "+1234567890"
  OPENAI_API_KEY: "your-openai-api-key"
